{
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.heyday.net.nz"
		}
	],
	"name": "heystack/ecommerce-core",
	"type": "silverstripe-module",
	"require": {
		"php": ">=5.4.0",
		"composer/installers": "~1.0",
		"heystack/heystack": "~3.0@dev"
	},
	"require-dev": {
		"silverstripe/framework": "~3.1@dev",
		"symfony/class-loader": "~2.2",
		"phpmd/phpmd": "1.4.*",
		"pdepend/pdepend": "1.1.*",
		"sami/sami": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Heystack\\Ecommerce\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0.x-dev",
			"dev-1.0": "1.0.x-dev"
		}
	}
}